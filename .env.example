# API Settings
API_KEY=your_api_key_here
ENV=dev # dev or pro
VALIDATE_API=True # True or False. If ENV=pro VALIDATE_API must be True
JWKS_HG_APP_URL=https://your-jwks-app-url.com
JWKS_HG_DEV_URL=https://your-jwks-dev-url.com
JWT_ISSUER_HG_APP=your-jwt-issuer-app
JWT_ISSUER_HG_DEV=your-jwt-issuer-dev
JWT_ALGORITHM=RS256

# OpenAI Settings
OPENAI_CHAT_MODEL=gpt-4
OPENAI_CODE_MODEL=gpt-4
OPENAI_TEXT_EMBEDDING_MODEL=text-embedding-ada-002
OPENAI_API_KEY=sk-your-openai-api-key

# Google AI Settings
GOOGLEAI_MODEL=gemini-pro
GOOGLEAI_MODEL_THINKING=gemini-pro
GOOGLEAI_MODEL_EDITOR=gemini-pro
GOOGLEAI_API_KEY=your-google-ai-api-key

# Qdrant Settings
QDRANT_BASE_COLLECTION_NAME=your_collection_name
QDRANT_URL=http://localhost:6333
QDRANT_HOST=localhost
QDRANT_PORT=6333

# LanceDB Settings
LANCEDB_PATH=./lancedb
LANCEDB_COLLECTION_NAME=your_lancedb_collection

# MongoDB Settings
MONGODB_CONNECTION_STRING=mongodb://localhost:27017
MONGODB_BASE_DATABASE_NAME=your_database_name
MONGODB_BASE_CHAT_HISTORY_COLLECTION_NAME=chat_history
MONGODB_BASE_DOC_COLLECTION_NAME=documents

# Prompt Paths
VAHACHA_CHATBOT_PROMPT_PATH=./prompts/chatbot_prompt.txt
VAHACHA_AGENT_PROMPT_PATH=./prompts/agent_prompt.txt
VAHACHA_AGENT_THINKING_PROMPT_PATH=./prompts/agent_thinking_prompt.txt
BASE_PROMPT_PATH=./prompts/base_prompt.txt

# SQL Database
SQL_DB_PATH=./database.db

# Base Query
BASE_QUERY=your_base_query_here

# Discord Bot Settings
DISCORD_BOT_TOKEN=your-discord-bot-token
DISCORD_BOT_URL=https://discord.com/api/v10
DISCORD_BOT_ID=your-bot-id
DISCORD_BOT_RATE_LIMIT_PERIOD=60
DISCORD_BOT_MAX_MESSAGES_PER_PERIOD=10
DISCORD_BOT_COOLDOWN_MESSAGE=Please wait before sending another message
DISCORD_BOT_NUM_WORKERS=4

# Langfuse Settings
LANGFUSE_PUBLIC_KEY=your-langfuse-public-key
LANGFUSE_SECRET_KEY=your-langfuse-secret-key
LANGFUSE_HOST=https://cloud.langfuse.com
LANGFUSE_TRACING_ENVIRONMENT=development
